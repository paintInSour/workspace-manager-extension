{
  "manifest_version": 3,
  "name": "Workspace Manager",
  "version": "1.0.0",
  "description": "Replace your new tab page with a productivity workspace featuring HotLinks and TodoList",
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon32.png",
    "128": "icons/icon128.png"
  }
}